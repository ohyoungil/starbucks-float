,"sources":{"main.scss":"/* COMMON */\r\nbody {\r\n  color: #333;\r\n  font-size: 16px;\r\n  font-weight: 400;\r\n  line-height: 1.4;\r\n  font-family: 'Nanum Gothic', sans-serif;\r\n  box-sizing: border-box;\r\n}\r\n\r\nimage {\r\n  display: block;\r\n}\r\n\r\na {\r\n  text-decoration: none;\r\n}\r\n\r\ninput {\r\n  outline: none;\r\n  border: none;\r\n}\r\n\r\n.inner {\r\n  width: 1100px;\r\n  margin: 0 auto;\r\n  position: relative;\r\n}\r\n\r\n/*#################### Header ####################*/\r\nheader > .inner {\r\n  height: 120px;\r\n}\r\nheader {\r\n  width: 100%;\r\n  background-color: #f6f5f0;\r\n  border-bottom: 1px soild #c8c8c8;\r\n  position: fixed;\r\n  top: 0;\r\n  z-index: 10;\r\n  .inner {\r\n    .logo {\r\n      height: 75px;\r\n      position: absolute;\r\n      top: 0;\r\n      bottom: 0;\r\n      margin: auto;\r\n    }\r\n    .sub-menu {\r\n      position: absolute;\r\n      right: 0px;\r\n      top: 10px;\r\n      overflow: hidden;\r\n      .menu {\r\n        float: left;\r\n        font-family: Arial, sans-serif;\r\n        overflow: hidden;\r\n        li {\r\n          float: left;\r\n          font-size: 12px;\r\n          padding: 11px 16px;\r\n          position: relative;\r\n          a {\r\n            color: #656565;\r\n          }\r\n          a:hover {\r\n            color: #333;\r\n            transition: 0.4s;\r\n          }\r\n        }\r\n        li::before {\r\n          content: '';\r\n          width: 1px;\r\n          height: 12px;\r\n          background-color: #e5e5e5;\r\n          position: absolute;\r\n          left: 0;\r\n        }\r\n        li:first-child::before {\r\n          display: none;\r\n        }\r\n      }\r\n      .search {\r\n        float: right;\r\n        position: relative;\r\n        cursor: pointer;\r\n        input {\r\n          width: 36px;\r\n          height: 34px;\r\n          font-size: 12px;\r\n          border: 1px solid #ccc;\r\n          border-radius: 8px;\r\n          transition: 0.4s;\r\n          border: 1px solid #ccc;\r\n        }\r\n        .material-icons {\r\n          position: absolute;\r\n          height: 24px;\r\n          right: 10px;\r\n          top: 0;\r\n          bottom: 0;\r\n          margin: auto;\r\n          transition: 0.4s;\r\n        }\r\n      }\r\n      // focused\r\n      .search.focused {\r\n        position: relative;\r\n        input {\r\n          width: 190px;\r\n          border-color: #669900;\r\n        }\r\n        .material-icons {\r\n          opacity: 0;\r\n        }\r\n      }\r\n    }\r\n    .main-menu {\r\n      position: absolute;\r\n      width: 100%;\r\n      right: 0;\r\n      bottom: 0;\r\n      font-family: Arial, sans-serif;\r\n      .item {\r\n        position: relative;\r\n        padding: 10px 20px 34px 20px;\r\n        margin: 0;\r\n        font-size: 13px;\r\n        float: left;\r\n        left: 350px;\r\n        .item__contents {\r\n          position: fixed;\r\n          width: 100%;\r\n          top: 120px;\r\n          left: 0;\r\n          background-color: #2c2a29;\r\n          display: none;\r\n          .contents__menu {\r\n            overflow: hidden;\r\n            padding: 20px 0;\r\n            .inner > li {\r\n              float: left;\r\n            }\r\n            .inner {\r\n              li {\r\n                width: 220px;\r\n                h4 {\r\n                  padding: 3px 0 12px 0;\r\n                  color: #fff;\r\n                  font-size: 14px;\r\n                }\r\n                ul {\r\n                  padding: 0;\r\n                  li {\r\n                    padding: 5px 0;\r\n                    color: #999;\r\n                    font-size: 12px;\r\n                    cursor: pointer;\r\n                  }\r\n                  li:hover {\r\n                    color: #669900;\r\n                  }\r\n                }\r\n              }\r\n            }\r\n          }\r\n          .contents__texture {\r\n            background-image: url(./images/main_menu_pattern.jpg);\r\n            padding: 26px 0;\r\n            font-size: 12px;\r\n            .inner {\r\n              h4 {\r\n                color: #999;\r\n                font-weight: 700;\r\n              }\r\n              p {\r\n                color: #669900;\r\n                margin: 4px 0px 14px;\r\n              }\r\n            }\r\n          }\r\n        }\r\n      }\r\n      .item:hover {\r\n        background-color: #2c2a29;\r\n        color: #669900;\r\n        border-radius: 6px 6px 0px 0px;\r\n        .item__contents {\r\n          display: block;\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"30a75095ecd5e89b6ec61d1fbd8a8945","cacheData":{"env":{}}}